clc;
clear all;
close all;
a=imread("/MATLAB Drive/cameraman.jpg");
subplot(2,2,1);
imshow(a);
title('original image');
b=255-a;
subplot(2,2,2);
imshow(b);
title('negative image');
l=255;
d=l/log10(1+l);
e=d*log10(1+double(a));
f=uint8(e);
subplot(2,2,3);
imshow(f);
title('log transform');
gamma=1.1;
g=double(a).^gamma;
subplot(2,2,4);
imshow(uint8(g));
title('power law transform');
