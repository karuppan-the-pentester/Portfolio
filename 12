#include "VSK_6748.h"
#include<math.h>
#include<stdio.h>
#define samples 10
#define l 2
float y[l*samples];
void main()
{
    float x[samples]={1,2,3,4,5,6,7,8,9,0};
    int i,j,k,cc,c,z;
    for(i=samples;i<(samples);i++)
    {
        x[i]=0;
    }
    for(i=0;i<l*samples;i++)
    {
        y[i]=0;
    }
    c=l-1;
    j=0;
    for(i=0;i<samples;i++)
    {
        y[i+j]=x[i];
        cc=i+j;
        z=i;
        for(k=0;k<c;k++)
        {
            y[cc+1]=0;
            i=i+1;
            cc=cc+1;
        }
        j=cc-z;
        i=i-c;
    }
}

