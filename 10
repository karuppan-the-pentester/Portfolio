#include<stdio.h>
#define LENGTH1 6
#define LENGTH2 4
int x[9]={1,3,4,5,6,0,0,0,0};
int h[9]={1,2,3,4,0,0,0,0,0};
int y[9];
main()
{
    int i=0,j;
    for(i=0;i<(LENGTH1+LENGTH2-1);i++)
    {
        y[i]=0;
        for(j=0;j<=i;j++)
            y[i]+=x[j]*h[i-j];
    }
    for(i=0;i<(LENGTH1+LENGTH2-1);i++)
        printf("%d\n",y[i]);
}
